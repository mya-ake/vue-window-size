{
  "name": "vue-window-size",
  "version": "1.1.2",
  "description": "Reactivity window size for Vue.js.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/mya-ake/vue-window-size.git",
  "bugs": {
    "url": "https://github.com/mya-ake/vue-window-size/issues"
  },
  "author": "mya-ake <tatsuya@neko-note.help>",
  "license": "MIT",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "vitest --dir tests/unit --run",
    "test:integration": "vitest --dir tests/integration --run",
    "release": "np",
    "prepare": "husky install"
  },
  "dependencies": {
    "window-resize-subject": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-dom": "^3.2.45",
    "@vue/runtime-core": "^3.2.45",
    "@vue/test-utils": "^2.2.7",
    "happy-dom": "^8.1.3",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "np": "^7.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.0",
    "vue": "^3.2.45"
  },
  "files": [
    "dist",
    "option-api.js",
    "index.d.ts",
    "option-api.d.ts"
  ],
  "keywords": [
    "vue.js",
    "window size"
  ],
  "lint-staged": {
    "*.{js,ts,json,md,yml}": "prettier --write"
  }
}
